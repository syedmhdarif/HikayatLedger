{
  "name": "hikayat-ledger",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "clean": "watchman watch-del-all && rm -rf node_modules && rm -rf ./android/app/build && rm -rf ./ios/build && rm -rf ./ios/Pods ./ios/Podfile.lock && yarn && yarn pod && yarn pod:update",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf Pods && rm -rf build && rm -rf Podfile.lock && cd .. && yarn install && yarn pod && yarn ios",
    "clean:all": "yarn clean:android && yarn clean:ios",
    "pod:update": "cd ios && pod install --repo-update && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "cache:clear": "rm -rf node_modules/.cache && rm -rf android/app/build && rm -rf android/build && rm -rf android/app/.cxx"
  },
